{
  "name": "app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf node_modules doc dist && npm cache clean",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean-install && npm start",
    "watch": "webpack --watch --progress --profile",
    "build": "rimraf dist && webpack --optimize --progress --profile --bail",
    "build-staging": "rimraf dist && webpack --config config/webpack.staging.js --optimize --progress --profile --bail --colors",
    "build-prod": "rimraf dist && webpack --config config/webpack.prod.js --optimize --progress --profile --bail --colors",
    "start": "webpack-dev-server --inline --progress --port 8080",
    "start-staging": "npm run build-staging && node server.js",
    "docs": "typedoc --options typedoc.json src/app/app.component.ts",
    "statistic": "webpack --profile --json > stats.json"
  },
  "engines": {
    "node": "5.2.0"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/router-deprecated": "2.0.0-rc.1",
    "core-js": "2.4.1",
    "ng2-bootstrap": "1.0.17",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "0.6.12",
    "reflect-metadata": "0.1.3",
    "basscss": "^8.0.1",
    "bootstrap-daterangepicker": "2.1.22",
    "copy-webpack-plugin": "^3.0.0",
    "css-loader": "^0.23.0",
    "classlist.js": "1.1.20150312",
    "express": "4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.4",
    "html-webpack-plugin": "^2.8.1",
    "json-loader": "^0.5.3",
    "jquery": "3.1.0",
    "throttle-debounce": "1.0.1",
    "moment": "2.14.1",
    "ng2-select": "1.0.3",
    "node-sass": "4.5.2",
    "normalize.css": "^4.1.1",
    "npm": "^3.9.3",
    "postcss-loader": "^0.9.1",
    "raw-loader": "0.5.1",
    "rimraf": "^2.5.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.13.0",
    "svg-sprite-loader": "0.0.19",
    "ts-helpers": "^1.1.1",
    "typedoc": "^0.3.12",
    "typescript": "^1.8.0",
    "typings": "^1.0.4",
    "webpack": "^1.12.13",
    "webpack-merge": "0.14.1",
    "tslint": "^3.4.0",
    "tslint-loader": "^2.1.0",
    "ts-loader": "^0.8.1"
  },
  "devDependencies": {
    "webpack-dev-server": "^1.14.1"
  }
}
